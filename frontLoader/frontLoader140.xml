            <actionSound template="hydraulicLayerTone2"   pitchScale="2.5" volumeScale="0.4" linkNode="hydraulicFront" actionNames="TRANSLATING_POS TRANSLATING_NEG" nodes="hydraulicFront" fadeIn="0.1" fadeOut="0.05"/>
