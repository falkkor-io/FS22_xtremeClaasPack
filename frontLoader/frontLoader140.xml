                <dependentPart node="angleDisplayPart02" />
            </movingTool>
            <movingTool node="supportArm">
                <dependentPart node="supportArmPart" />
            </movingTool>
        </movingTools>

        <movingParts>
            <movingPart node="attacherRotDummy" referenceFrame="attacherRootDummy">
