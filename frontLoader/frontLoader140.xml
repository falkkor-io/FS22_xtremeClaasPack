            <sound template="clackVar1"  startTime="0.29" direction="0" volumeScale="0.7" pitchScale="2"   linkNode="supportArm"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="armDummy" delayedNode="arm">
